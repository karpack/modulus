{
    "name": "karpack/modulus",
    "description": "Package supporting module based architecture",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/karpack/modulus/issues",
        "source": "https://github.com/karpack/modulus"
    },
    "keywords": ["laravel", "modulus"],
    "authors": [
        {
            "name": "Kalesh Kaladharan",
            "email": "kalesh.13@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": ">=7.0",
        "karpack/contracts": "^1.0",
        "symfony/finder": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Karpack\\Modulus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Karpack\\Modulus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
